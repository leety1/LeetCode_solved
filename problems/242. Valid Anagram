class Solution {
    public boolean isAnagram(String s, String t) {
        char[] a = new char[26];
        char[] b = new char[26];
        for(int i = 0; i< s.length(); ++i) a[i] = s.charAt(i);
        for(int i = 0; i< t.length(); ++i) b[i] = t.charAt(i);
        Arrays.sort(a); Arrays.sort(b);

        if(Arrays.equals(a,b)) return true;
        else return false;
    }
}
